{
  "name": "orion-vscode",
  "displayName": "Orion",
  "publisher": "SaiYs",
  "version": "0.1.0",
  "description": "A customizable vscode theme, built on `Deno` and `Radix Colors`.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SaiYs/Orion.git"
  },
  "preview": false,
  "categories": ["Themes"],
  "keywords": [],
  "extensionKind": ["ui", "workspace"],
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "main": "dist/main.js",
  "activationEvents": ["onStartupFinished"],
  "contributes": {
    "configuration": {
      "title": "Orion",
      "properties": {
        "orion-vscode.accentColor": {
          "default": "indigo",
          "enum": [
            "tomato",
            "red",
            "ruby",
            "crimson",
            "pink",
            "plum",
            "purple",
            "violet",
            "iris",
            "indigo",
            "blue",
            "cyan",
            "teal",
            "jade",
            "green",
            "grass",
            "orange",
            "brown",
            "sky",
            "mint",
            "lime",
            "yellow",
            "amber",
            "gold",
            "bronze",
            "gray",
            "mauve",
            "slate",
            "sage",
            "olive",
            "sand"
          ],
          "type": "string",
          "markdownDescription": "Controls accent color. Reloading window will be required to apply update to the current theme"
        },
        "orion-vscode.neutralGray": {
          "default": false,
          "type": "boolean",
          "markdownDescription": "Use neutral grayscale color"
        }
      }
    },
    "themes": [
      {
        "label": "Orion",
        "uiTheme": "vs-dark",
        "path": "./dist/themes/orion-color-theme.json"
      }
    ]
  }
}

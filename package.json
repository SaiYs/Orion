{
  "name": "orione",
  "displayName": "Orion",
  "publisher": "SaiYs",
  "version": "0.0.1",
  "description": "A set of themes for VSCode with customizable accent colors, based on `Radix Colors`: A gorgeous, accessible color system for user interfaces.",
  "license": "MIT",
  "repository": "https://github.com/SaiYs/Orion.git",
  "preview": false,
  "categories": [
    "Themes"
  ],
  "keywords": [],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "main": "dist/main.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "Orion",
      "properties": {
        "orion.accentColor": {
          "default": "indigo",
          "enum": [
            "tomato",
            "red",
            "ruby",
            "crimson",
            "pink",
            "plum",
            "purple",
            "violet",
            "iris",
            "indigo",
            "blue",
            "cyan",
            "teal",
            "jade",
            "green",
            "grass",
            "orange",
            "brown",
            "sky",
            "mint",
            "lime",
            "yellow",
            "amber",
            "gold",
            "bronze",
            "gray",
            "mauve",
            "slate",
            "sage",
            "olive",
            "sand"
          ],
          "markdownDescription": "Controls accent color. Reloading window will be required to apply update to the current theme",
          "type": "string"
        }
      }
    },
    "themes": [
      {
        "label": "Orion",
        "uiTheme": "vs-dark",
        "path": "./dist/themes/orion-color-theme.json"
      }
    ]
  }
}
